{% extends 'base.html.twig' %}

{% block title %}Liste des rhums{% endblock %}

{% block body %}
    <h1>Liste des rhums</h1>

    <ul>
        {% for rhum in rhums %}
            <li>
                {{ rhum.marque }}
                <a href="{{ path('app_rhum_show', {id:rhum.id}) }}">Fiche rhum</a>

            </li>
        {% endfor %}
    </ul>

{% endblock %}

